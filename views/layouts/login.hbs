	<div class="limiter">
		<div class="container-login100" style="background-image: url('assets/images/bg-01.jpg');">
			<div class="wrap-login100 p-l-110 p-r-110 p-t-62 p-b-33">
				<form class="login100-form validate-form flex-sb flex-w" method="POST" action="/api/auth/login">
					<span class="login100-form-title p-b-53">
						Iniciar Sesión Con
					</span>

					<a href="/auth/github" class="btn-github m-b-20">
						<img style="width: 37px;" src="https://cdn-icons-png.flaticon.com/512/25/25231.png" alt="GITHUB">
						GitHub
					</a>

					<a href="/auth/twitter" class="btn-google m-b-20">
						<img src="https://www.edigitalagency.com.au/wp-content/uploads/Twitter-logo-png.png" alt="TWITTER">
						Twitter
					</a>
					
					<div class="p-t-31 p-b-9">
						<span class="txt1">
							Email
						</span>
					</div>
					<div class="wrap-input100 validate-input" data-validate = "Ingrese su Email">
						<input class="input100" type="email" name="email" >
						<span class="focus-input100"></span>
					</div>
					
					<div class="p-t-13 p-b-9">
						<span class="txt1">
							Contraseña
						</span>

						<a href="#" class="txt2 bo1 m-l-5">
							¿La ha olvidado?
						</a>
					</div>
					<div class="wrap-input100 validate-input" data-validate = "Ingrese su Contraseña">
						<input class="input100" type="password" name="user_password" >
						<span class="focus-input100"></span>
					</div>

					<div class="container-login100-form-btn m-t-17">
						<button class="login100-form-btn">
							<span class="spinner-border spinner-border-sm" id="spinnerBtn" style="height: 30px;width: 30px;display: none;" role="status" aria-hidden="true"></span>
							<span id="spanBtnText">Iniciar sesión</span>
						</button>
					</div>

					<div class="w-full text-center p-t-55">
						<span class="txt2">
							¿No tienes cuenta?
						</span>

						<a href="/register" class="txt2 bo1">
							Regístrate ahora
						</a>
					</div>
				</form>
			</div>
		</div>
	</div>
	
